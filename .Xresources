! urxvt {{{
! urxvt.depth: 32
! urxvt.fading: 3
! urxvt.fadeColor: #FFFFFF
urxvt.scrollBar: false
urxvt.internalBorder: 24
urxvt.lineSpace: 2
urxvt.iso14755: false
urxvt.font: xft:Source Code Pro:pixelsize=15,xft:TerminessTTFNerdFontMono:size=15
URxvt.letterSpace: -2
urxvt.boldFont:
urxvt.cursorBlink: true
! keybinds
urxvt.keysym.Home: \033[1~
urxvt.keysym.End: \033[4~
URxvt*saveLines: 200000
! end of urxvt }}}
